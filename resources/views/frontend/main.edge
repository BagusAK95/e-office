    <script>
    
    function cek_session()
    {
      $.ajax({ 
          type: 'GET', 
          url: '/cek_session', 
          success: function (data) { 
              if(data)
              {
                return false;
              }
              else
              {
                swal({title: "Session Time Out", text: "Silahkan login kembali.", type: "error"},
                function(){ 
                    window.location.href='/login';
                }
                );
              }
          }
      });
    }

    function cek_token()
    {
        $.ajax({ 
            type: 'GET', 
            url: '/gettoken', 
            success: function (data) { 
                $.ajax({ 
                    type: 'GET', 
                    url: '/api/profile', 
                    headers: {
                        'Authorization': 'Bearer ' + data.token
                    },
                    success: function (data) { 
                        //alert(data.data.level);
                        $("#level_main").val(data.data.level);
                        $("#nip_login").val(data.data.nip);
                        //putlevel(data.data.level)
                        $(".nama_user").html(data.data.nama_lengkap).show();
                        if(data.data.foto)
                        {
                          $(".foto").html('<img src="'+data.data.foto+'" style="width:70px">').show();
                        }
                        else
                        {
                          $(".foto").html('<img src="/material/base/src/images/user.png" style="width:70px">').show();
                        }
                        
                       
                        if(data.data.level==1)
                        {
                          var lv = "Admin";
                        }
                        else if(data.data.level==2)
                        {
                          var lv = "Pimpinan";
                        }
                        else if(data.data.level==3)
                        {
                          var lv = "Tata Usaha";
                        }
                        else if(data.data.level==4)
                        {
                          var lv = "Staff";
                        }

                        $(".jabatan").html(lv).show();
                        $(".jabatanm").html(lv).show();

                        if(data.data.level==1)
                        {
                          $(".konsol").show();
                          $(".surat").hide();
                        }
                        else
                        {
                          $(".konsol").hide();
                          $(".surat").show();
                        }


                        var akses = data.data.akses;
                        var smu = "";
                        if(akses)
                        {
                            var max = akses.split(",").length;
                            var data_akses = akses.split(",");
                            for (var index = 0; index < max; index++) 
                            {
                                if(data_akses[index]=="suratmasuk")
                                {
                                  $("#surat_masuk").show();
                                }
                                if(data_akses[index]=="suratkeluar")
                                {
                                  $("#surat_keluar").show();
                                }
                                if(data_akses[index]=="disposisi")
                                {
                                  $("#disposisi").show();
                                  $("#disposisi_keluar").show();
                                }
                            }
                        }
                    }
                });
            },
            error: function ()
            {
                return false;
            }
            });
    }
  </script>

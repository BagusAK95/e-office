    <script>
      cek_session()
      function cek_session()
        {
          $.ajax({ 
              type: 'GET', 
              url: '/admin/cek_session', 
              success: function (data) { 
                  if(data)
                  {
                    return false;
                  }
                  else
                  {
                    swal({title: "Session Time Out", text: "Silahkan login kembali.", type: "error"},
                    function(){ 
                        window.location.href='/login';
                    }
                    );
                  }
              }
          });
        }

        cek_token()
        function cek_token()
        {
            $.ajax({ 
                type: 'GET', 
                url: '/admin/gettoken', 
                success: function (data) { 
                    $.ajax({ 
                        type: 'GET', 
                        url: '/api/profile', 
                        headers: {
                            'Authorization': 'Bearer ' + data.token
                        },
                        success: function (data) { 
                            $(".nama_user").html(data.data.nama_lengkap).show();
                            $(".foto").html('<img src="'+data.data.foto+'">').show();
                            $(".jabatan").html(data.data.nama_jabatan).show();
                        }
                    });
                },
                error: function ()
                {
                    return false;
                }
                });
        }
        
        
      </script>
